@model HostClientApplication.Models.LocalBooking

@{
    ViewData["Title"] = "Edit";
}

<div id="FlorimJobbarMycket" class="container">

    <header>
        <h2>Redigera bokningar för lokaler</h2><a href="javascript:void(0)" onclick="SubmitProfile()">Spara</a>
    </header>
    <div class="eventuppgifter">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="profileform"></div>
            <div class="input-box">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control" />
            </div>
            <div class="input-box">
                <label asp-for="StartDateTime" class="control-label"></label>
                <input asp-for="StartDateTime" class="form-control" />
            </div>
            <div class="input-box">
                <label asp-for="EndDateTime" class="control-label"></label>
                <input asp-for="EndDateTime" class="form-control" />
            </div>
            <div class="input-box">
                <label asp-for="Category" class="control-label"></label>
                <input asp-for="Category" class="form-control" />
            </div>
            <div class="input-box">
                <label asp-for="LocalId" class="control-label"></label>
                <input asp-for="LocalId" class="form-control" />
            </div>
            <div class="input-box">
                <label asp-for="EventId" class="control-label"></label>
                <input asp-for="EventId" class="form-control" />
            </div>
            <div class="input-box">
                <label asp-for="HostId" class="control-label"></label>
                <input asp-for="HostId" class="form-control" />
            </div>
            <div class="input-box">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

