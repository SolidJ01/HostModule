@model Event

@{
    ViewData["Title"] = "Edit";
}

<div id="FlorimJobbarMycket" class="container">
    <header>
        <h2>Skapa/Redigera Event</h2><a href="javascript:void(0)" onclick="SubmitProfile()">Spara</a>
    </header>


    <form id="profileform" action="/Event/Index" method="post" enctype="multipart/form-data">
        <div class="eventuppgifter">
            <div class="input-box">

                <input type="text" placeholder="Namn" required>
            </div>
            <div class="input-box">

                <input type="text" placeholder="Datum" required>
            </div>
            <div class="input-box">

                <input type="text" placeholder="Starttid" required>
            </div>
            <div class="input-box">

                <input type="text" placeholder="Sluttid" required>
            </div>
            <div class="input-box">

                <input type="text" placeholder="Stad" required>
            </div>
            <div class="input-box">

                <input type="text" placeholder="Venue" required>
            </div>
            <div class="input-box">

                <input type="text" placeholder="Bild" required>
            </div>

            <div id="Beskrivning" contenteditable="true">

                <input id="Beskrivninghidden" type="hidden" name="beskrivningAvArrangör" placeholder="Beskrivning" required />

            </div>



        </div>
    </form>



</div>