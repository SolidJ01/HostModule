<section class="logincontainer">
    <form action="/Login/Index" method="post" enctype="multipart/form-data">
        <div>
            <h2>Inloggning</h2>
        </div>
        @ViewBag.Message
        <input type="text" name="loginId" placeholder="LoginId" />
        <input type="text" name="name" placeholder="Name" />
        <input type="text" name="phone" placeholder="Phone" />
        <input type="text" name="description" placeholder="LoginId" />
        <input type="text" name="website" placeholder="Website" />
        <input type="text" name="name" placeholder="Username" />
        <input type="password" name="password" placeholder="Lösenord" />
        <input type="text" name="accesslevel" placeholder="AccessLevel" />
        <input type="text" name="userId" placeholder="UserId" />

        <input type="submit" value="Registrera Dig" />
    </form>
</section>

